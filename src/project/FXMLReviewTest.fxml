<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: azure;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.FXMLReviewTestController">
   <children>
      <Label fx:id="lbQuestionText" layoutX="39.0" layoutY="77.0" prefHeight="168.0" prefWidth="630.0" text="Question text" textFill="#2d13f0" wrapText="true">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label fx:id="lbChoiceA" layoutX="107.0" layoutY="249.0" prefHeight="42.0" prefWidth="503.0" text="ChoiceA" wrapText="true" />
      <Label fx:id="lbChoiceB" layoutX="107.0" layoutY="307.0" prefHeight="42.0" prefWidth="503.0" text="ChoiceB" wrapText="true" />
      <Label fx:id="lbChoiceC" layoutX="107.0" layoutY="361.0" prefHeight="42.0" prefWidth="503.0" text="ChoiceC" wrapText="true" />
      <Label fx:id="lbChoiceD" layoutX="107.0" layoutY="417.0" prefHeight="42.0" prefWidth="503.0" text="ChoiceD" wrapText="true" />
      <Button fx:id="btOptionA" disable="true" layoutX="66.0" layoutY="258.0" mnemonicParsing="false" text="A" />
      <Button fx:id="btOptionB" disable="true" layoutX="66.0" layoutY="315.0" mnemonicParsing="false" text="B" />
      <Button fx:id="btOptionC" disable="true" layoutX="66.0" layoutY="370.0" mnemonicParsing="false" text="C" />
      <Button fx:id="btOptionD" disable="true" layoutX="65.0" layoutY="426.0" mnemonicParsing="false" text="D" />
