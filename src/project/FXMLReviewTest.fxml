      <Label fx:id="lbQuestionNum" layoutX="79.0" layoutY="495.0" text="Question # / #" textFill="#2d13f0" />
      <Label fx:id="lbFinishTest" layoutX="161.0" layoutY="592.0" prefHeight="42.0" prefWidth="394.0" text="Return to your account page" wrapText="true" />
      <Label fx:id="lbTitle" layoutX="190.0" layoutY="23.0" text="Test Review Mode" textFill="#df0d0d">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Text layoutX="281.0" layoutY="539.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: orange;" text="Your Answer">
         <font>
            <Font size="16.0" />
         </font>
      </Text>
      <Text layoutX="385.0" layoutY="539.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: green;" text="Correct Answer">
         <font>
            <Font size="16.0" />
         </font>
      </Text>
   </children>    
</AnchorPane>
